    "guid-typescript": "^1.0.9",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/cookie": "^0.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^14.18.35",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "proxy-agent": "^5.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.25.1",
    "serverless-domain-manager": "4.1.0",
    "typescript": "4.9.4"
  }
