{
  "name": "cookieproxy",
  "version": "4.0.0",
  "description": "An AWS API Gateway authorizer, as the entry point for cookie based API requests from the SPA",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "rimraf dist && tsc --sourceMap true",
    "setup": "./test/setup.sh",
    "reverseProxy": "sls invoke local -f reverseProxy",
    "lambda": "./test/lambda.sh",
    "package": "npm run buildRelease && rimraf .serverless && sls package --stage deployed",
    "buildRelease": "rimraf dist && tsc",
    "deploy": "sls deploy --stage deployed --package .serverless",
    "remove": "sls remove --stage deployed"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-cors": "^2.5.3",
    "base64url": "^3.0.1",
    "cookie": "^0.4.1",
    "fs-extra": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/cookie": "^0.4.0",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "rimraf": "^3.0.2",
    "serverless": "^2.71.0",
    "serverless-domain-manager": "4.1.0",
    "typescript": "4.4.3"
  }
}
