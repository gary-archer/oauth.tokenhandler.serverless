  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "rimraf dist && tsc",
    "buildRelease": "rimraf dist && tsc --sourceMap false",
    "start": "bash ./start.sh",
    "test": "bash ./start.sh",
    "deployDev": "bash ./deploy.sh dev",
    "deploy": "bash ./deploy.sh deployed",
    "httpDev": "bash ./test/runHttpWorkflow.sh dev",
    "http": "bash ./test/runHttpWorkflow.sh deployed",
    "removeDev": "sls remove --stage dev",
    "remove": "sls remove --stage deployed"
  },
  "dependencies": {
    "@middy/core": "^4.0.8",
    "axios": "^1.2.1",
    "base64url": "^3.0.1",
