{
  "name": "tokenhandler",
  "version": "1.0.0",
  "description": "Supporting components to manage cookie routing for an SPA that uses API driven OpenID Connect",
  "scripts": {
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "rimraf dist && tsc --sourceMap true",
    "buildRelease": "rimraf dist && tsc",
    "http": "./test/runHttpWorkflow.sh",
    "lambda": "./test/runLambdaWorkflow.sh",
    "packageConfig": "./environments/packageConfig.sh",
    "restoreConfig": "./environments/restoreConfig.sh",
    "packageLocal": "npm run buildRelease && rimraf .serverless && sls package --stage local",
    "deployLocal": "sls deploy --stage local --package .serverless",
    "removeLocal": "sls remove --stage local",
    "package": "npm run buildRelease && rimraf .serverless && sls package --stage deployed",
    "deploy": "sls deploy --stage deployed --package .serverless --aws-profile default",
    "remove": "sls remove --stage deployed"
  },
  "author": "Gary Archer",
  "license": "MIT",
  "dependencies": {
    "@middy/core": "^2.5.3",
    "axios": "^0.27.2",
    "base64url": "^3.0.1",
    "cookie": "^0.4.1",
    "fs-extra": "^9.0.1",
    "guid-typescript": "^1.0.9",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/cookie": "^0.4.0",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "proxy-agent": "^5.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.18.2",
    "serverless-domain-manager": "4.1.0",
    "serverless-plugin-scripts": "^1.0.2",
    "typescript": "4.7.2"
  }
}
